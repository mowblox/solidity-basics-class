pragma solidity ^0.8.4;

contract BasicOperations {
    // math operations
    uint x = 100 - 50; // subtraction
    uint y = 100 + 22; // addition
    uint z = 100 / 20; // division
    uint m = 5 * 5; // multiplication

    // boolean logic operations
    bool val = 100 < 50; // less than
    bool xal = 100 > 50; // greater than
    bool ral = 100 <= 50; // less than or equal to
    bool tal = 100 >= 50; // greater than or equal to
    bool qua = 100 == 100; // equality
    bool nqua = 100 != 50; // non-equality
    bool nega = !true; // logical negation
    bool and = false && false; // logical AND
    bool lor = true || false; // logical OR
    

    // other operations
    uint rem = 100 % 30; // modulo or remainder

    bytes b;

}